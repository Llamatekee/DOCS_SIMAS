<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="gramatica.xsl"?>
<grammar version="2.0">
	<name>RecFac</name>
	<description>Gramática diseñada para qur cntenga revursivadad y neecesite ser factorizada</description>
	<non-terminal-symbols>
		<non-terminal>
			<value>S</value>
		</non-terminal>
	</non-terminal-symbols>
	<terminal-symbols>
		<terminal>
			<value>a</value>
		</terminal>
		<terminal>
			<value>b</value>
		</terminal>
		<terminal>
			<value>c</value>
		</terminal>
		<terminal>
			<value>d</value>
		</terminal>
		<terminal>
			<value>e</value>
		</terminal>
		<terminal>
			<value>f</value>
		</terminal>
	</terminal-symbols>
	<init-symbol>S</init-symbol>
	<rule-set>
		<rule>
			<leftPart>
				<value>S</value>
			</leftPart>
			<rightPart>
				<symbol>
					<value>S</value>
				</symbol>
				<symbol>
					<value>a</value>
				</symbol>
			</rightPart>
		</rule>
		<rule>
			<leftPart>
				<value>S</value>
			</leftPart>
			<rightPart>
				<symbol>
					<value>b</value>
				</symbol>
				<symbol>
					<value>S</value>
				</symbol>
				<symbol>
					<value>c</value>
				</symbol>
			</rightPart>
		</rule>
		<rule>
			<leftPart>
				<value>S</value>
			</leftPart>
			<rightPart>
				<symbol>
					<value>d</value>
				</symbol>
				<symbol>
					<value>e</value>
				</symbol>
			</rightPart>
		</rule>
		<rule>
			<leftPart>
				<value>S</value>
			</leftPart>
			<rightPart>
				<symbol>
					<value>d</value>
				</symbol>
				<symbol>
					<value>f</value>
				</symbol>
			</rightPart>
		</rule>
	</rule-set>
</grammar>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" styleClass="wizard-step" xmlns:fx="http://javafx.com/fxml">
    <padding>
        <Insets top="20" right="20" bottom="20" left="20"/>
    </padding>

    <Label text="Producciones Originales" styleClass="wizard-header"/>
    
    <ListView fx:id="listViewProducciones" VBox.vgrow="ALWAYS" styleClass="wizard-list"/>
    
    <HBox alignment="CENTER">
        <Button text="Cerrar" styleClass="button-cancel"/>
    </HBox>
</VBox> 